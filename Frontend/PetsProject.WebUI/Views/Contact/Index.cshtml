
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="zxx">

@await Component.InvokeAsync("_HeadPartial")

<body id="top-header">
    @if (User.Identity.IsAuthenticated)
    {
        @await Html.PartialAsync("/Views/MemberLayout/MemberHeaderPartial.cshtml")
    }
    else
    {
        @await Component.InvokeAsync("_ContactHeaderPartial")
    }
  

    @await Html.PartialAsync("/Views/Contact/SendMessage.cshtml")

    @await Component.InvokeAsync("_FooterPartial")

    @await Component.InvokeAsync("_ButtonTopPartial")

    @await Component.InvokeAsync("_ContactScriptsPartial")


</body>
</html>


